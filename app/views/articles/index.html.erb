<h1>Articles</h1>

<p><%= link_to 'Create new article', new_article_path %></p>

<table>
<thead>
<tr><th>Title</th><th>Description</th></tr>
</thead>
<tbody>
<% @articles.each do |article| %>
  <tr><td><%= link_to article.title, article_path(article) %></td><td><%= article.description %></td></tr>
<% end %>
</tbody>
</table>
