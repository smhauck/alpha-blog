<h1 align='center'>Listing All Categories</h1>
<div align='center'>
  <%= will_paginate %>
  <% @categories.each do |cat| %>
    <ul class='listing'>
      <div class='row'>
        <div class='well col-md-4 col-md-offset-4'>
          <li class='article-title'>
            <%= link_to "#{cat.name}", category_path(cat) %>
          </li>
        </div>
      </div>
    </ul>
  <% end %>
  <%= will_paginate %>
</div>
